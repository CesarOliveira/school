{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The Root Schema",
    "description": "The root schema comprises the entire JSON document.",
    "required": [
        "course"
    ],
    "properties": {
        "course": {
            "$id": "#/properties/course",
            "type": "object",
            "title": "The Course Schema",
            "description": "An explanation about the purpose of this instance.",
            "default": {},
            "examples": [
                {
                    "id": 5.0,
                    "updated_at": "2020-03-18T11:34:35.450Z",
                    "name": "Course Name",
                    "details": "Nice Details about it",
                    "created_at": "2020-03-18T11:34:35.450Z"
                }
            ],
            "required": [
                "id",
                "name",
                "details",
                "created_at",
                "updated_at"
            ],
            "properties": {
                "id": {
                    "$id": "#/properties/course/properties/id",
                    "type": "integer",
                    "title": "The Id Schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": 0,
                    "examples": [
                        5
                    ]
                },
                "name": {
                    "$id": "#/properties/course/properties/name",
                    "type": "string",
                    "title": "The Name Schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": "",
                    "examples": [
                        "Course Name"
                    ]
                },
                "details": {
                    "$id": "#/properties/course/properties/details",
                    "type": "string",
                    "title": "The Details Schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": "",
                    "examples": [
                        "Nice Details about it"
                    ]
                },
                "created_at": {
                    "$id": "#/properties/course/properties/created_at",
                    "type": "string",
                    "title": "The Created_at Schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": "",
                    "examples": [
                        "2020-03-18T11:34:35.450Z"
                    ]
                },
                "updated_at": {
                    "$id": "#/properties/course/properties/updated_at",
                    "type": "string",
                    "title": "The Updated_at Schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": "",
                    "examples": [
                        "2020-03-18T11:34:35.450Z"
                    ]
                }
            }
        }
    }
}
